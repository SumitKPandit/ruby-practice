<h1><%= @company.name %></h1>
<h2><%= @company.ticker_symbol %></h2>
<h3><%= @company.risk_factor %></h3>
<hr>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Price</th>
    </tr>
  </thead>
  <tbody>
    <% @stock_prices.each do |sp| %>
      <tr>
        <td><%= sp.captured_at %></td>
        <td><%= sp.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>
